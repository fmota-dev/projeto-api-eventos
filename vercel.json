{
	"version": 2,
	"builds": [
		{
			"src": "src/app.js",
			"use": "@now/node-server"
		},
		{
			"src": "public/(.*)",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "(.*)",
			"dest": "src/app.js"
		},
		{
			"src": "/public/(.*)",
			"dest": "/public/$1"
		}
	]
}
